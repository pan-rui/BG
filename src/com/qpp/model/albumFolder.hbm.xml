<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping>
	<class name="com.qpp.model.AlbumFolder" table="qb_photo_album_folder">
		<id name="id" column="folderId">
			<generator class="native"/>
		</id>
		<property name="name" column="folderName" type="string"/>
		<property name="folderRemark" column="folderRemark" type="string"/>
		<property name="folderParentId" column="folderParentId" type="java.lang.Long" insert="false" update="false"/>
		<property name="userId" column="userId" type="java.lang.Long"/>
		<property name="albumId" column="albumId" type="java.lang.Long"/>
		<property name="createDate" column="createDate" type="java.util.Date"/>
		<property name="modifyDate" column="modifyDate" type="java.util.Date"/>
		<many-to-one name="parent" class="com.qpp.model.AlbumFolder" column="folderParentId"/>
		<set name="children" cascade="save-update" inverse="true">
			<key column="folderParentId"/>
			<one-to-many class="com.qpp.model.AlbumFolder"/>
		</set>
		<set name="pictures">
			<key column="folderId"/>
			<one-to-many class="com.qpp.model.PictureInfo"/>
		</set>
	</class>
</hibernate-mapping> 