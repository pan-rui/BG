<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping>
	<class name="com.qpp.model.PictureInfo" table="qb_photo_album_detail_new">
		<id name="photoId" column="photoId" type="string"/>
		<property name="name" column="photoName" type="string"/>
		<property name="photoExp" column="photoExp" type="string"/>
		<property name="folderId" column="folderId" type="java.lang.Long"/>
		<property name="userId" column="userId" type="java.lang.Long"/>
		<property name="width" column="photoWidth" type="java.lang.Integer"/>
		<property name="height" column="photoHeight" type="java.lang.Integer"/>
		<property name="photoRemark" column="photoRemark" type="string"/>
		<property name="createDate" column="createDate" type="java.util.Date"/>
		<property name="modifyDate" column="modifyDate" type="java.util.Date"/>
		<set name="tags" table="qb_photo_tag">
			<key column="photoId"/>
			<many-to-many class="com.qpp.model.Tag" column="tagId"/>
		</set>
	</class>
</hibernate-mapping> 