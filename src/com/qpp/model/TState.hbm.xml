<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.qpp.model.TState" table="qb_country_state_info">
        <composite-id mapped="true" class="com.qpp.model.TStatePK">
            <key-property name="countryCode" column="countryCode"/>
            <key-property name="stateCode" column="stateCode"/>
        </composite-id>
        <property name="stateDesc" column="stateDesc"/>
    </class>
</hibernate-mapping>