<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.qpp.model.TState" table="qb_country_state_info">
        <composite-id>
            <key-property name="countryCode" column="countryCode" type="java.lang.String"/>
            <key-property name="stateCode" column="stateCode" type="java.lang.String"/>
        </composite-id>
        <property name="stateDesc" column="stateDesc"/>
        <property name="direction" column="Direction"/>
    </class>
</hibernate-mapping>