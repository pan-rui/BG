<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping>
	<class name="com.qpp.model.FontInfo" table="t_font">
		<id name="fontCode" column="fontCode" type="string"/>
		<property name="fontName" column="fontName" type="string"/>
		<property name="hasAlignCenter" column="hasAlignCenter"/>
		<property name="hasAlignRight" column="hasAlignRight"/>
		<property name="hasNormal" column="hasNormal"/>
		<property name="hasBold" column="hasBold"/>
		<property name="hasItalic" column="hasItalic"/>
		<property name="realItalic" column="realItalic" type="string"/>
		<property name="italicRotate" column="italicRotate" type="string"/>
		<property name="italicSpace" column="italicSpace" type="string"/>
		<property name="allowChar" column="allowChar" type="string"/>
		<property name="status" column="status"/>
	</class>
</hibernate-mapping> 