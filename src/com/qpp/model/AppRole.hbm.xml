<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.qpp.model.AppRole" table="t_app_role">
        <composite-id>
            <key-property name="appid" column="appid"/>
            <key-property name="levelid" column="levelid"/>
        </composite-id>
    </class>
</hibernate-mapping>